mkdir -p /proc
mkdir -p /tmp
mkdir -p /sys
mkdir -p /mnt
/bin/mount -a
mkdir -p /dev/pts
mount -t devpts devpts /dev/pts
echo /sbin/mdev > /proc/sys/kernel/hotplug
mdev -s

echo "4 4 1 7" > /proc/sys/kernel/printk
/bin/hostname -F /etc/hostname
PATH=/sbin:/bin:/usr/sbin:/usr/bin
mount -t ext2 /dev/mmcblk0 /mnt
